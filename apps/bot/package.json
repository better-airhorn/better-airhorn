		"@types/mkdirp": "^1.0.0",
		"@types/node": "^14.0.23",
		"@types/node-fetch": "^2.5.7",
		"@types/ws": "^7.2.4"
	},
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
