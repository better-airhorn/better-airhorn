		"url": "https://github.com/Clemens-E/better-airhorn/issues"
	},
	"dependencies": {
		"@better-airhorn/audio": "^0.0.2",
		"@better-airhorn/entities": "^0.0.2",
		"@better-airhorn/shori": "^0.0.1",
		"@better-airhorn/structures": "^0.0.1",
		"@discordjs/opus": "^0.3.2",
		"@rocka/mp3-duration": "^2.0.0",
		"@types/ms": "^0.7.31",
		"await-lock": "^2.0.1",
		"bufferutil": "^4.0.1",
		"bull": "^3.15.0",
		"common-tags": "^1.8.0",
